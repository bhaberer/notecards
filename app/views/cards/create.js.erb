if (<%= @card.valid? %>) { 
  $('#new_card').slideUp();
  $('#card .card').fadeOut(function() { 
    $('#card .card').remove();
  });
  if (<%= params[:yesterday].present? %>) { 
    window.location.reload();
  } else { 
    #('#card').append("<%= escape_javascript( render :partial => 'card' ) %>");
  }
} else { 
  $('#new_card').replaceWith("<%= escape_javascript( render 'form' ) %>");
   moveErrors();
}
